{
    "manifest_version": 2,
    "name": "AutoClick Extension",
    "version": "1.0",
    "permissions": ["activeTab"],
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_end"
      }
    ]
  }
  